<Window x:Class="ДорожноеПриложение.MInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ДорожноеПриложение"
        mc:Ignorable="d"
        Title="Убавить материал" Height="150" Width="200"
        MaxHeight="150" MinHeight="150"
        MaxWidth="200" MinWidth="200"
        ResizeMode="NoResize">
    <StackPanel>
        <TextBlock x:Name="MaterialBox" Margin="0,20,0,5"/>
        <TextBox x:Name="PlisBox" Margin="0,5"/>
        <Button x:Name="OkBtn" Content="OK" Click="OkBtn_Click"  Margin="80,5,0,0"/>
    </StackPanel>
</Window>
